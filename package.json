{
  "name": "@shakuroinc/figma-extractor",
  "version": "3.3.2",
  "author": "Shakuro team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakurocom/figma-extractor"
  },
  "bin": {
    "figma-extract": "./dist/bin.js"
  },
  "types": "src/types.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "src/types.ts"
  ],
  "scripts": {
    "dev": "ts-node ./src/bin.ts",
    "build": "rm -rf dist && yarn tsc",
    "lint": "eslint . --ext .ts,.tsx,.js --ignore-path .gitignore",
    "test": "jest",
    "prepare": "husky install",
    "release": "yarn build && git push --tags && npm publish --access public --new-version"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cosmiconfig": "^8.2.0",
    "figma-js": "^1.16.0",
    "shelljs": "^0.8.5",
    "svg-sprite-generator": "^0.0.7",
    "svgo": "^3.0.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@shakuroinc/eslint-config-base": "1.3.0",
    "@types/jest": "^29.5.3",
    "@types/shelljs": "^0.8.12",
    "@types/svgo": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "memfs": "^4.2.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && lint-staged"
    }
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.10"
  }
}
